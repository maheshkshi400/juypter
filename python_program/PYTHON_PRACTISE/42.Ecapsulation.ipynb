    "# getter\n",
    "\n",
    "class student:\n",
    "    def __init__(self):\n",
    "        self.__name = \"\"\n",
    "\n",
    "    def get_name(self):\n",
    "        return self.__name\n",
    "\n",
    "    def set_name(self, name):\n",
    "        self.__name = name\n",
    "\n",
    "obj = student()\n",
    "obj.set_name(\"John\")\n",
    "print(obj.get_name())  # Output: John"
   ]
